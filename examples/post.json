{
  "url": "http://localhost:3000/user",
  "method": "POST",
  "duration": 1,
  "rps": {
    "value": 1,
    "step": 2,
    "interval": 10
  },
  "body": {
    "value": {
      "company": "Microsoft"
    },
    "dynamic": [
      {
        "key": "name",
        "type": "string",
        "length": 12
      },
      {
        "key": "age",
        "type": "number",
        "range": [20, 50]
      },
      {
        "key": "city",
        "type": "string",
        "values": ["New-York", "London", "Barcelona"]
      }
    ]
  }
}
